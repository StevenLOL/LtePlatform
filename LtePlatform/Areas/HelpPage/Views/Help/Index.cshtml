
@{
    ViewBag.Title = "ASP.NET Web API Help Page";
    
}

<header class="help-page">
    <div class="content-wrapper">
        <div class="float-left">
            <h1>@ViewBag.Title</h1>
        </div>
    </div>
</header>
<div id="body" class="help-page">
    <section class="featured">
        <div class="content-wrapper">
            <h2>{{page.title}}</h2>
            <p>
                {{page.introduction}}
            </p>
        </div>
    </section>
    <section class="content-wrapper main-content clear-fix">
        <div ng-view></div>
    </section>
</div>

@section Scripts{
    <link rel="stylesheet" href="~/Areas/HelpPage/HelpPage.css"/>
    <script src="~/Areas/HelpPage/Scripts/route.js"></script>
}
