<!DOCTYPE html>
<html lang="zh">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - 佛山网优</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/Content/bootstrap-theme.min.css"/>
    <link rel="stylesheet" href="~/Content/Site.min.css"/>
    @Scripts.Render("~/bundles/modernizr")
</head>
<body ng-app="myApp">
    <div class="navbar navbar-blue-azure navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img width="60" src="~/Content/Images/Logo_new_version.png" ng-click="sideBarShown=!sideBarShown" />
            </div>
            <div class="navbar-collapse collapse">
                <div ng-include="'/appViews/HeaderMenu.html'"></div>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2004 &nbsp;-&nbsp; @DateTime.Now.Year - 中国电信佛山分公司欧阳晖</p>
        </footer>
    </div>

<script src="~/Scripts/jquery-3.1.0.min.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
<link href="~/Content/ui-bootstrap-csp.css" rel="stylesheet"/>
<link href="~/Content/ui-grid.min.css" rel="stylesheet"/>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
<script src="~/Scripts/angular-ui-router.min.js"></script>
<script src="~/Scripts/angular-animate.min.js"></script>
<script src="~/Scripts/angular-touch.min.js"></script>

    <script src="~/Scripts/angular-ui/csv.js"></script>
    <script src="~/Scripts/angular-ui/pdfmake.js"></script>
    <script src="~/Scripts/angular-ui/vfs_fonts.js"></script>
<script src="~/Scripts/angular-ui/ui-bootstrap-tpls.min.js"></script>
<script src="~/Scripts/ui-grid.min.js"></script>

<script src="~/Scripts/highcharts/highcharts.js"></script>
<script src="~/Scripts/highcharts/highcharts-3d.js"></script>
<script src="~/Scripts/highcharts/highcharts-more.js"></script>
<script src="~/Scripts/highcharts/modules/exporting.js"></script>
<script src="~/Scripts/highcharts/modules/drilldown.js"></script>
<script src="~/Scripts/highcharts/modules/data.js"></script>
    <script src="~/Scripts/highcharts/highcharts-ng.js"></script>
    <script src="~/Scripts/mycharts/comboChart.js"></script>
    <script src="~/Scripts/mycharts/drilldown.chart.js"></script>
    <script src="~/Scripts/mycharts/column3d.js"></script>
    <script src="~/Scripts/mycharts/gaugeMeter.js"></script>
    <script src="~/Scripts/service/app.kpi.service.js"></script>
    <script src="~/Scripts/service/app.region.service.js"></script>
<script src="~/Scripts/service/app.url.service.js"></script>
<script src="~/Scripts/service/topic/baidu.map.js"></script>
<script src="~/Scripts/service/kpi/workitem.service.js"></script>
    <script src="~/Scripts/service/mongo/cell.huawei.js"></script>
    <script src="~/Scripts/service/mongo/handoff.js"></script>
    <script src="~/Scripts/service/topic/college.js"></script>
<script src="~/Scripts/service/filters/handoff.js"></script>
<script src="~/Scripts/service/filters/basic.js"></script>
<script src="~/Scripts/service/filters/cell.js"></script>
<script src="~/directives/app/app.module.js"></script>
<script src="~/directives/rutrace/rutrace.module.js"></script>
<script src="~/directives/parameters/parameters.module.js"></script>
<script src="~/directives/workitem/workitem.module.js"></script>
<script src="~/directives/customer/customer.module.js"></script>
<script src="~/directives/college/college.module.js"></script>
    <script src="~/Scripts/app/common.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
